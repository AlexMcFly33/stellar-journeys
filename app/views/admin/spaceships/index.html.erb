<h1>Welcome to your profile</h1>


<div class="container-spaceship">
  <h3 class="mt-4">Your Spaceship :</h3>
    <% @spaceships.each do |spaceship| %>
    <div class="card-product mt-3">
    <img src="<%= cl_image_path spaceship.photo.key %>" />
    <div class="card-product-infos">
      <h2><%= spaceship.name %></h2>
      <p><%= link_to '🗑️', admin_spaceship_path(spaceship), data: { turbo_method: :delete, turbo_confirm: "Are you sure?" }, class: "link" %> - <%= link_to '📝', edit_admin_spaceship_path(spaceship), class: "link" %></p>
    </div>
  </div>
  <% end %>
